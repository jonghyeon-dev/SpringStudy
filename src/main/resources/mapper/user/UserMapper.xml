<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sarang.mapper.user.UserMapper">
	<select id="getUserInfo" resultType="com.sarang.model.user.TADM00100VO">
		SELECT * FROM TADM00100;
	</select>
	<select id="searchUserInfo" resultType="com.sarang.model.user.TADM00100VO">
		SELECT * FROM TADM00100
		WHERE 1=1
		  <if test='seq != null and !seq.equals("")'>
		  AND SEQ LIKE CONCAT('%',#{seq},'%')
		  </if>
		  <if test='eno != null and !eno.equals("")'>
		  AND ENO LIKE CONCAT('%',#{eno},'%')
		  </if>
	</select>
</mapper>